{
  "manifest_version": 3,
  "name": "FileTrace (Yandex)",
  "version": "0.2.0",
  "action": {
    "default_title": "FileTrace",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
